        "Price": {
            "id": "Price",
            "properties": {
                "amount": {
                    "type": "string"
                },
                "currency": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "Product": {
            "id": "Product",
            "properties": {
                "attributes": {
                    "$ref": "Attributes"
                },
                "contentLanguage": {
                    "type": "string"
                },
                "destinationStatuses": {
                    "items": {
                        "$ref": "DestinationStatus"
                    },
                    "type": "array"
                },
                "issues": {
                    "items": {
                        "$ref": "Issue"
                    },
                    "type": "array"
                },
                "name": {
                    "type": "string"
                },
                "parent": {
                    "type": "string"
                },
                "productId": {
                    "type": "string"
                },
                "targetCountry": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "ProductDetail": {
            "id": "ProductDetail",
            "properties": {
                "attributeName": {
                    "type": "string"
                },
                "attributeValue": {
                    "type": "string"
                },
                "sectionName": {
                    "type": "string"
                }
            },
            "type": "object"
        }
    },
