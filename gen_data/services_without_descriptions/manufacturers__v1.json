                },
                "disclosureDate": {
                    "type": "string"
                },
                "excludedDestination": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "featureDescription": {
                    "items": {
                        "$ref": "FeatureDescription"
                    },
                    "type": "array"
                },
                "flavor": {
