        "PatchJob": {
            "id": "PatchJob",
            "properties": {
                "createTime": {
                    "format": "google-datetime",
                    "type": "string"
                },
                "displayName": {
                    "type": "string"
                },
                "dryRun": {
                    "type": "boolean"
                },
                "duration": {
                    "format": "google-duration",
                    "type": "string"
                },
                "errorMessage": {
                    "type": "string"
