                "instanceFilter": {
                    "$ref": "PatchInstanceFilter"
                },
                "name": {
                    "type": "string"
                },
                "patchConfig": {
                    "$ref": "PatchConfig"
                },
                "patchDeployment": {
                    "type": "string"
                },
